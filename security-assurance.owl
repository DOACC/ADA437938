    <!-- Property Declarations -->

    <owl:DatatypeProperty rdf:about="&security-assurance;byOrganization">
        <rdfs:domain rdf:resource="&security-assurance;Assurance"/>
        <rdfs:range rdf:resource="&xsd;string"/>
